<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>msgs</var>
		</resource-bundle>
	</application>
	<!-- <managed-bean> -->
	<!-- <managed-bean-name>login</managed-bean-name> -->
	<!-- <managed-bean-class>com.sdi.presentation.BeanLogin</managed-bean-class> -->
	<!-- <managed-bean-scope>session</managed-bean-scope> -->
	<!-- </managed-bean> -->
	<managed-bean>
		<managed-bean-name>users</managed-bean-name>
		<managed-bean-class>com.sdi.presentation.BeanUsers</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>tasks</managed-bean-name>
		<managed-bean-class>com.sdi.presentation.BeanTasks</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>settings</managed-bean-name>
		<managed-bean-class>com.sdi.presentation.BeanSettings</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/*</from-view-id>
		<navigation-case>
			<from-action>#{tasks.listInbox}</from-action>
			<to-view-id>/restricted/principalUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.listToday}</from-action>
			<to-view-id>/restricted/listTodayTasks.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.listWeek}</from-action>
			<to-view-id>/restricted/listWeekTasks.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{login.cerrarSesion}</from-action>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{login.verify}</from-action>
			<from-outcome>client</from-outcome>
			<to-view-id>/restricted/principalUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{login.verify}</from-action>
			<from-outcome>admin</from-outcome>
			<to-view-id>/restricted/principalAdmin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{login.verify}</from-action>
			<from-outcome>fracaso</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>registrarse</from-outcome>
			<to-view-id>/altaForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/principalAdmin.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{users.listado}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/listUsers.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{users.resetDatabase}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/principalAdmin.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/principalUser.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{tasks.addTarea}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/addTaskForm.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.selectTask(vtask)}</from-action>
			<from-outcome>edit</from-outcome>
			<to-view-id>/restricted/editTaskForm.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.setFinishedInInbox(vtask)}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/principalUser.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/listUsers.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{users.resetDatabase}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/principalAdmin.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/addTaskForm.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{tasks.atras}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/principalUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.salva}</from-action>
			<from-outcome>inbox</from-outcome>
			<to-view-id>/restricted/principalUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.salva}</from-action>
			<from-outcome>today</from-outcome>
			<to-view-id>/restricted/listTodayTasks.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.salva}</from-action>
			<from-outcome>week</from-outcome>
			<to-view-id>/restricted/listWeekTasks.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.atras}</from-action>
			<from-outcome>fracaso</from-outcome>
			<to-view-id>/restricted/addTaskForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/altaForm.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{users.salva}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{users.salva}</from-action>
			<from-outcome>fracaso</from-outcome>
			<to-view-id>/altaForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/altaForm.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{users.salva}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{users.salva}</from-action>
			<from-outcome>fracaso</from-outcome>
			<to-view-id>/altaForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/listTodayTasks.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{tasks.setFinishedInToday(vtask)}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/listTodayTasks.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.selectTask(vtask)}</from-action>
			<from-outcome>edit</from-outcome>
			<to-view-id>/restricted/editTaskForm.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.addTarea}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/addTaskForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/listWeekTasks.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{tasks.setFinishedInWeek(vtask)}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/listWeekTasks.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.selectTask(vtask)}</from-action>
			<from-outcome>edit</from-outcome>
			<to-view-id>/restricted/editTaskForm.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.addTarea}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/addTaskForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/editTaskForm.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{tasks.salva}</from-action>
			<from-outcome>inbox</from-outcome>
			<to-view-id>/restricted/principalUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.salva}</from-action>
			<from-outcome>today</from-outcome>
			<to-view-id>/restricted/listTodayTasks.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.salva}</from-action>
			<from-outcome>week</from-outcome>
			<to-view-id>/restricted/listWeekTasks.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{tasks.atras}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/principalUser.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
